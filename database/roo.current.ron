                        "local argspec = List(vref):slice(2)",
                        "if #argspec > 0 then",
                        "  local pas = S.code_utils:parse_argspec(unpack(argspec))",
                        "  if type(pas) == \"table\" then",
                        "    if pas[2] and #pas[2] > 0 then",
                        "      player:tell(\'I don\\\'t understand \"%s\"\' % {S.string_utils:from_list(pas[2], \" \")})",
                        "      return",
                        "    end",
