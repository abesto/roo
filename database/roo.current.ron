                        "    --       return $failed_match;",
                        "    --     endif",
                        "    --     string = string[length(pn) + 2..$];",
                        "    --   endwhile",
                        "    --   if (object == #0 || typeof(object) == ERR)",
                        "    --     return $failed_match;",
                        "    --   else",
